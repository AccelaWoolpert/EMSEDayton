<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    <section name="HansenAccelaReqTypeMappings" 
             type="System.Configuration.NameValueFileSectionHandler,System, Version=1.0.3300.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
    <section name="HansenAccelaPriorityMappings"
           type="System.Configuration.NameValueFileSectionHandler,System, Version=1.0.3300.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
  </configSections>
  <HansenAccelaReqTypeMappings>
    <!---key is the Hansen Request Type Code / value is the Accela Complaint 1 value -->
    <add key="HIAF" value="ANIMAL FECES" />
    <add key="HITYNA" value="TRASH/DEBRIS ON PREMISES" />
    <add key="HIPITG" value="VEHICLE PARKING IN YARD" />
  </HansenAccelaReqTypeMappings>
  <HansenAccelaPriorityMappings>
    <!---key is the Hansen Priority Code / value is the Accela Priority value -->
    <add key="1" value="High" />
    <add key="2" value="Low" />
    <add key="3" value="Medium" />
    <add key="4" value="Medium" />
    <add key="4T" value="Medium" />
    <add key="5R" value="High" />
    <add key="9" value="Medium" />
  </HansenAccelaPriorityMappings>
  <log4net>
    <root>
      <level value="ALL" />
      <appender-ref ref="LogFileAppender" />
      <appender-ref ref="ColoredConsoleAppender" />
    </root>
    <logger name="NHibernate">
      <level value="WARN" />
    </logger>
    <logger name="NHibernate.SQL">
      <level value="WARN" />
    </logger>
    <appender name="ColoredConsoleAppender" type="log4net.Appender.ColoredConsoleAppender">
      <mapping>
        <level value="ERROR" />
        <foreColor value="Red, HighIntensity" />
      </mapping>
      <mapping>
        <level value="INFO" />
        <foreColor value="White" />
      </mapping>
      <mapping>
        <level value="DEBUG" />
        <foreColor value="Cyan" />
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%-5p %d{yyyy-MM-dd hh:mm:ss} [%thread] - %m%n" />
      </layout>
    </appender>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="wslogs\" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <staticLogFileName value="false" />
      <datePattern value="yyyy-MM-dd'.log'" />
      <maxSizeRollBackups value="15" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%-5p %d{yyyy-MM-dd hh:mm:ss} [%thread] - %m%n" />
      </layout>
    </appender>
  </log4net>
  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
    <add key="HansenBaseUri" value="http://wtrhan84ts/webservices"/>
    <add key="HansenProvider" value="han84"/>
    <add key="HansenWSUser" value="jlloyd"/>
    <add key="HansenWSPassword" value="hansen"/>
    <add key="AddedDateTime" value="08/01/2016"/>
    <add key="AddedByComment" value="Added by HansenAccelService"/>
    <add key="HansenDefaultMediumPriority" value="3"/>
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5"/>
  </system.web>
  <system.serviceModel>
    <services>
      <service name="HansenAccelaServices.HansenServiceRequests" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.IHansenServiceRequests" behaviorConfiguration="web">
        </endpoint>
      </service>
      <service name="HansenAccelaServices.HansenAuth" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.IHansenAuth" behaviorConfiguration="web">
        </endpoint>
      </service>
      <service name="HansenAccelaServices.HansenCustomerCall" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.IHansenCustomerCall" behaviorConfiguration="web">
        </endpoint>
      </service>
      <service name="HansenAccelaServices.HansenServiceRequestType" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.IHansenServiceRequestType" behaviorConfiguration="web">
        </endpoint>
      </service>
      <service name="HansenAccelaServices.HansenEmployee" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.IHansenEmployee" behaviorConfiguration="web">
        </endpoint>
      </service>
      <service name="HansenAccelaServices.HansenAccessUser" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.IHansenAccessUser" behaviorConfiguration="web">
        </endpoint>
      </service>
      <service name="HansenAccelaServices.HansenAddresses" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.IHansenAddresses" behaviorConfiguration="web">
        </endpoint>
      </service>
      <service name="HansenAccelaServices.HansenSetAccelaRecordId" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.IHansenSetAccelaRecordId" behaviorConfiguration="web">
        </endpoint>
      </service>
      <service name="HansenAccelaServices.HansenServiceRequestCreate" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.IHansenServiceRequestCreate" behaviorConfiguration="web">
        </endpoint>
      </service>
      <service name="HansenAccelaServices.LogTest" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.ILogTest" behaviorConfiguration="web">
        </endpoint>
      </service>
      <service name="HansenAccelaServices.TestMapping" behaviorConfiguration="ServiceBehavior">
        <endpoint address="" binding="webHttpBinding" contract="HansenAccelaServices.ITestMapping" behaviorConfiguration="web">
        </endpoint>
      </service>
    </services>
    <behaviors>
      <endpointBehaviors>
        <behavior name="web">
          <webHttp />
        </behavior>
      </endpointBehaviors>
      <serviceBehaviors>
        <behavior name="ServiceBehavior">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <protocolMapping>
        <!--<add binding="basicHttpsBinding" scheme="https" />-->
    </protocolMapping>    
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true" />
  </system.serviceModel>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
    <!--
        To browse web app root directory during debugging, set the value below to true.
        Set to false before deployment to avoid disclosing web app folder information.
      -->
    <directoryBrowse enabled="true"/>
  </system.webServer>

</configuration>
